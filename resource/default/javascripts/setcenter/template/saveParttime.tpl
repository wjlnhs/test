<div style="width:350px; padding:10px 20px 20px 20px;">
    <div class="sbox-tr p5-0">
        <div class="adpart-tt lt tright"><span class="red m-r5">*</span>所在组织：</div>
        <div class="sbox-td lt">
            {name}
        </div>
        <div class="clear"></div>
    </div>
    <div class="sbox-tr p5-0">
        <div class="adpart-tt lt tright"><span class="red m-r5">*</span>兼职角色名称：</div>
        <div class="sbox-td lt">
            <input class="add-part-name" type="text" value="{partName}" />
        </div>
        <div class="clear"></div>
    </div>
    <div class="sbox-tr p5-0">
        <div class="adpart-tt lt tright">兼职人员：</div>
        <div class="sbox-td lt">
            <input id="js_add_part_txt" class="" type="text" />
        </div>
        <div class="clear"></div>
    </div>
     <div class="sbox-tr p5-0">
            <div class="adpart-tt lt tright">所在职级：</div>
            <div class="sbox-td lt" id="classLineSelectPanel">

            </div>
            <div class="clear"></div>
        </div>
    <div class="tct m-t10">
        <div class="adpart-tt lt">&nbsp;</div>
        <span class="blue96x32 m-r10">保存</span>
        <span class="gray96x32">取消</span>
    </div>
</div>