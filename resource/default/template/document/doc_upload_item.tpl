<div class="edit-file-item"  ext="{Extension}" id="{id}">
    <i class="file-icon-{Extension} lt"></i>
    <div class="lt w-365 l-h30 m-l20">
        <span class="file-item-title">
            <a href="javascript:void(0)" class="inputspan">{name}</a>
            <input docid="160" value="{name}">
            <a class="btn-edit"></a>
        </span>
        <div class="black" class="percent" id="{id}percent">{percent}%</div>
    </div>
    <div class="rt m-t15"><a style="display:none" class="btn-delete2">删除</a></div>
    <div class="clear"></div>
</div>